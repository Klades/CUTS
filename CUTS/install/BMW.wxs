                  <File Id="Node_Details.ascx" Name="Node_Details.ascx"
                        Source="$(var.BMW_ROOT)\Node_Details.ascx"
                    DiskId="1" />

                  <!-- web services -->
                  <File Id="Service.asmx" Name="Service.asmx"
                        Source="$(var.BMW_ROOT)\Service.asmx" DiskId="1" />

                  <!-- config files -->
