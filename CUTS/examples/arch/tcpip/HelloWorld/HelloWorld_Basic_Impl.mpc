  sharedname   = HelloWorld_Basic_Impl

  dynamicflags = HELLOWORLD_BASIC_IMPL_BUILD_DLL

  prebuild = perl -- $(ACE_ROOT)/bin/generate_export_file.pl HELLOWORLD_BASIC_IMPL > $(PROJECT_ROOT)/HelloWorld_Basic_Impl_export.h
  after += HelloWorld_exec

  libs += HelloWorld_exec \
    HelloWorld_stub

