// -*- IDL -*-

#ifndef _HELLOWORLD_OSPL_IDL_
#define _HELLOWORLD_OSPL_IDL_

module CUTS_OSPL
{
  struct Message
  {
    // data member: time
    ::CUTS_OSPL::TimeValue time;

    // data member: content
    string content;
  };

  // define the keylist for the struct/event.
  #pragma keylist Message
  struct TimeValue
  {
    // data member: usec
    long usec;

    // data member: sec
    long sec;
  };
};

#endif  // _HELLOWORLD_OSPL_IDL_

