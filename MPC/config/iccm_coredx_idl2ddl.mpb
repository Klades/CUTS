// $Id:

project {
  specific {
    idl2ddl = $(CUTS_ROOT)/bin/iccm_coredx
  }

  Define_Custom(IDL2DDL) {
    command               = $(IDL2DDL)
    commandflags          = -Wb,generate_ddl
    dependent             = $(IDL2DDL)
    inputext              = .idl

    generic_pre_filename   = CoreDX_
    generic_outputext     = .ddl

    keyword iccm_coredx_idl2ddl_flags = commandflags
  }
}
