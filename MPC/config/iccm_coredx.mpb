// $Id$

project {
  specific {
    iccm_coredx = $(CUTS_ROOT)/bin/iccm_coredx
  }

  Define_Custom (ICCM) {
    command               = $(ICCM_COREDX)
    dependent             = $(ICCM_COREDX)
    inputext              = .idl

    source_outputext      = C.cpp
    header_outputext      = C.h
    source_pre_filename   = CoreDX_
    header_pre_filename   = CoreDX_
    generic_outputext     = _iCCM.idl

    keyword iccm_coredx_flags = commandflags
  }
}
