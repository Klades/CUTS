project {
  specific {
    iccm_tcpip = $(CUTS_ROOT)/bin/iccm_tcpip
  }

  Define_Custom (ICCM) {
    command               = $(ICCM_TCPIP)
    dependent             = $(ICCM_TCPIP)
    inputext              = .idl

    source_outputext      = C.cpp
    header_outputext      = C.h
    source_pre_filename   = TCPIP_
    generic_outputext     = _iCCM.idl

    keyword iccm_tcpip_flags = commandflags
  }
}
