// $Id$

project {
  specific {
    iccm_tao = $(CUTS_ROOT)/bin/iccm_tao
  }

  Define_Custom (ICCM) {
    command               = $(ICCM_TAO)
    dependent             = $(ICCM_TAO)
    inputext              = .idl

    source_outputext      = C.cpp
    header_outputext      = C.h
    source_pre_filename   = TAO_
    generic_outputext     = _iCCM.idl

    keyword iccm_tao_flags = commandflags
  }
}
