project {
  Define_Custom(Protoc) {
    command               = <%quote%>$(PROTOBUF_ROOT)/bin/protoc<%quote%>
    dependent             = <%quote%>$(PROTOBUF_ROOT)/bin/protoc<%quote%>

    commandflags          = --cpp_out=.
    inputext              = .proto

    source_outputext      = .pb.cc
    header_outputext      = .pb.h

    keyword cuts_protoc_flags = commandflags
  }
}
