// $Id$

project : requires_opensplice {
  specific {
    iccm_ospl = $(CUTS_ROOT)/bin/iccm_ospl
  }

  Define_Custom (ICCM) {
    command               = $(ICCM_OSPL)
    dependent             = $(ICCM_OSPL)
    inputext              = .idl

    source_outputext      = C.cpp
    header_outputext      = C.h
    source_pre_filename   = OpenSplice_
    generic_outputext     = _iCCM.idl

    keyword iccm_ospl_flags = commandflags
  }
}
