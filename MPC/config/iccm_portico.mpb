// $Id$

project {
  specific {
    iccm_portico = $(CUTS_ROOT)/bin/iccm_portico
  }

  Define_Custom (ICCM) {
    command               = $(ICCM_PORTICO)
    dependent             = $(ICCM_PORTICO)
    inputext              = .idl

    source_outputext      = C.cpp
    header_outputext      = C.h
    source_pre_filename   = Portico_
    generic_outputext     = _iCCM.idl

    keyword iccm_portico_flags = commandflags
  }
}
