  dynamicflags += ICCM_TRON_DEPLOYMENT_HANDLERS_STUB_BUILD_DLL

  after += iCCM_Tron_Deployment_Handlers_Idl_gen

  Source_Files {
    Tron_Deployment_HandlerC.cpp
  }

  Template_Files {

  }

  Header_Files {

  }

  Inline_Files {

  }
}

